import java.util.ArrayList;

public class CustomerDaoTest {
	public static void main(String[] args) {
		CustomerDao dao = CustomerDao.getInstance();
		int result;
		ArrayList<CustomerDto> customers;
		System.out.println("1. 회원가입 테스트");
		String ctel = "010-5555-5555"; String cname = "윤길동";
		result = dao.insertCustomer(ctel, cname);
		System.out.println(result == CustomerDao.SUCCESS ? cname + "님 회원가입 감사. 포인트 1000점 부여": cname+"님 회원가입 실패입니다");
		;
	}
}
